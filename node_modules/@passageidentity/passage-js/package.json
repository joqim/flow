{
  "name": "@passageidentity/passage-js",
  "version": "3.1.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit & tsup ./src/index.ts --no-config  --sourcemap --format esm,cjs --dts --minify && npm run build:cdn",
    "build:cdn": "tsup",
    "build:watch": "tsc --noEmit & tsup ./src/index.ts --watch --no-config --format esm,cjs --dts --minify",
    "build:watch:be_services_local": "npm run build:be_services_local && npm run build:watch",
    "build:watch:dev": "npm run build:dev && npm run build:watch",
    "build:be_services_local": "echo 'export default {URL: \"http://localhost:3004\", version: \"'$npm_package_name' '$npm_package_version'\"};' > ./src/config/env_config.ts && npm run build",
    "build:dev": "npm run config:dev && npm run build",
    "build:local": "npm run config:local && npm run build",
    "build:one-passage": "npm run config:one-passage && npm run build",
    "build:prod": "npm run config:prod && npm run build",
    "build:uat": "npm run config:uat && npm run build",
    "config:dev": "env-cmd -f ./.env.dev ./scripts/build-config.sh",
    "config:local": "env-cmd -f ./.env.local ./scripts/build-config.sh",
    "config:one-passage": "env-cmd -f ./.env.one-passage ./scripts/build-config.sh",
    "config:prod": "env-cmd -f ./.env.prod ./scripts/build-config.sh",
    "config:uat": "env-cmd -f ./.env.uat ./scripts/build-config.sh",
    "deploy:cloud-storage:dev": "gsutil cp ./cdn/index.js gs://cdn.passage.dev/passage-js.js",
    "deploy:cloud-storage:uat": "gsutil cp ./cdn/index.js gs://cdn-uat.passage.dev/passage-js.js",
    "deploy:cloud-storage:prod": "gsutil cp ./cdn/index.js gs://cdn.passage.id/passage-js.js",
    "clean:dist": "rm -rf dist",
    "clean:node_modules": "rm -rf node_modules",
    "commit:pre": "npm run lint && npm run format && git add -A .",
    "format": "pretty-quick --staged",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "lint:fix": "eslint 'src/**/*.ts?(x)' --fix",
    "serve": "npm run build:watch:dev",
    "serve:be_services_local": "npm run build:watch:be_services_local",
    "sentry:release:dev": "env-cmd -f ./.env.dev ./scripts/sentry-release.sh",
    "sentry:release:prod": "env-cmd -f ./.env.prod ./scripts/sentry-release.sh",
    "sentry:release:uat": "env-cmd -f ./.env.uat ./scripts/sentry-release.sh"
  },
  "keywords": [],
  "author": "Passage Identity, Inc.",
  "license": "ISC",
  "devDependencies": {
    "@types/base-64": "^1.0.0",
    "@types/js-cookie": "^3.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "beachball": "^2.21.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "lint-staged": "^12.4.0",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@types/node": "^17.0.27",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "serialize-error": "^11.0.0"
  }
}
